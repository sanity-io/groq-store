{
  "name": "@sanity/groq-store",
  "version": "1.0.0",
  "description": "Stream dataset to memory for in-memory querying",
  "keywords": [
    "sanity",
    "memory",
    "query",
    "groq"
  ],
  "homepage": "https://github.com/sanity-io/groq-store#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/groq-store/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/groq-store.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/typings/index.d.ts",
      "source": "./src/index.ts",
      "browser": "./dist/browser/groq-store.modern.mjs",
      "deno": "./dist/browser/groq-store.modern.mjs",
      "node": "./dist/node/groq-store.js",
      "require": "./dist/node/groq-store.js",
      "default": "./dist/browser/groq-store.modern.mjs"
    }
  },
  "main": "dist/node/groq-store.js",
  "module": "dist/browser/groq-store.modern.mjs",
  "browser": "dist/browser/groq-store.js",
  "typings": "dist/typings/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:node && npm run build:browser",
    "build:browser": "microbundle -i src/browser/index.ts -o dist/browser -f modern,cjs --tsconfig tsconfig.build.json",
    "build:node": "microbundle -i src/index.ts -o dist/node -f cjs --target=node --tsconfig tsconfig.build.json",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm run build && npm run test ",
    "start": "cd example && npm start",
    "test": "tsdx test"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@sanity/types": "^2.35.0",
    "eventsource": "^1.1.2",
    "fast-deep-equal": "^3.1.3",
    "groq": "^2.33.2",
    "groq-js": "^1.1.0",
    "mendoza": "^2.1.1",
    "simple-get": "^4.0.1",
    "split2": "^4.1.0",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@sanity/client": "^2.23.1",
    "@sanity/semantic-release-preset": "^2.0.0",
    "@types/eventsource": "^1.1.9",
    "@types/node": "^18.11.7",
    "@types/split2": "^3.2.1",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.2",
    "lint-staged": "^12.4.1",
    "microbundle": "^0.15.0",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "rimraf": "^3.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
