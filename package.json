{
  "name": "@sanity/memquery",
  "version": "1.0.0",
  "description": "Stream dataset to memory for in-memory querying",
  "main": "dist/node/memquery.js",
  "browser": "dist/browser/memquery.js",
  "module": "dist/browser/memquery.modern.js",
  "typings": "dist/node/node/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "start": "tsdx watch",
    "prebuild": "rimraf dist",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "microbundle -i src/node/index.ts -o dist/node -f cjs --target=node",
    "build:browser": "microbundle -i src/browser/index.ts -o dist/browser -f modern,cjs",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/memquery.git"
  },
  "keywords": [
    "sanity",
    "memory",
    "query",
    "groq"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/memquery/issues"
  },
  "homepage": "https://github.com/sanity-io/memquery#readme",
  "dependencies": {
    "@sanity/types": "^2.0.1",
    "eventsource": "^1.0.7",
    "fast-deep-equal": "^3.1.3",
    "groq": "^2.0.1",
    "groq-js": "^0.1.8",
    "mendoza": "^2.0.1",
    "split2": "^3.2.2",
    "throttle-debounce": "^2.3.0"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.4",
    "@types/split2": "^2.1.6",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^6.0.0",
    "eslint-config-sanity": "^1.150.8",
    "eslint-plugin-react": "^7.21.4",
    "microbundle": "^0.12.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  }
}
